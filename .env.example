# Server Configuration
PORT=3000
NODE_ENV=development

# Cequence AI Gateway (Required for MCP Security)
CEQUENCE_API_KEY=your_cequence_api_key_here
CEQUENCE_BASE_URL=https://api.cequence.ai
CEQUENCE_PROXY_ENDPOINT=your_proxy_endpoint_here

# Descope Authentication (Required for Security)
DESCOPE_PROJECT_ID=your_descope_project_id_here
DESCOPE_MANAGEMENT_KEY=your_descope_management_key_here

# GitHub Integration
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
GITHUB_TEST_TOKEN=your_github_personal_access_token_here

# Slack Integration
SLACK_CLIENT_ID=your_slack_client_id_here
SLACK_CLIENT_SECRET=your_slack_client_secret_here
SLACK_TEST_TOKEN=your_slack_bot_token_here
SLACK_TEST_CHANNEL=your_slack_channel_id_here

# Notion Integration
NOTION_CLIENT_ID=your_notion_client_id_here
NOTION_CLIENT_SECRET=your_notion_client_secret_here
NOTION_DEFAULT_DATABASE_ID=your_notion_database_id_here


# Google Calendar Integration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback
GOOGLE_CALENDAR_TEST_TOKEN=ya29.your_access_token_here

# Test Settings
TEST_EMAIL_1=your_test_email_1@gmail.com
TEST_EMAIL_2=your_test_email_2@gmail.com
AUTO_DELETE_TEST_EVENTS=true
TEST_REPO=owner/repo
TEST_PR=123

# Development & Debugging
LOG_LEVEL=debug
ENABLE_REQUEST_LOGGING=true

