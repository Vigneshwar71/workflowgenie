# Environment configuration for Smithery deployment

# Server Configuration
PORT=3000
NODE_ENV=production
TRANSPORT=http

# MCP Configuration - Only required settings for Smithery
CEQUENCE_API_KEY=${CEQUENCE_API_KEY}
DESCOPE_PROJECT_ID=${DESCOPE_PROJECT_ID}
DESCOPE_MANAGEMENT_KEY=${DESCOPE_MANAGEMENT_KEY}

# Optional - For demo purposes, stub these if not available
GITHUB_CLIENT_ID=smithery_demo
SLACK_CLIENT_ID=smithery_demo
GOOGLE_CLIENT_ID=smithery_demo
NOTION_CLIENT_ID=smithery_demo

# Development & Debugging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true
